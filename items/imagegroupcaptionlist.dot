{{# def.partials.formGroup }}
    {{# def.partials.label }}
    <ul ml-bind="{{= it.compClass}}:{{= it.compName }}">
        <li ml-bind="[item]:result">
            <div class="img-group-item">
                <img ml-bind="[data]:hostUrl">
            </div>
            <div class="img-group-item">
                <textarea ml-bind="[data]:caption" class="form-control ml-ui-textarea"></textarea>
            </div>
        </li>
    </ul>
</div>
