{{# def.partials.formGroup }}
  <label>
    <input type="checkbox"
      id="{{= it.compName }}"
      ml-bind="MLInput:{{= it.compName }}"
      {{? it.disabled }}disabled {{?}}
      {{? it.checked }}checked {{?}}
      class="{{= it.item.itemCssClass || ''}}">
    {{= it.item.label}}
  </label>
  {{# def.partials.tooltip }}
</div>
