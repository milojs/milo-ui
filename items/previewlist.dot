{{# def.partials.formGroup }}
    {{# def.partials.label }}
    <div ml-bind="CCPreviewList:{{= it.compName }}">
        <div ml-bind="[list]:list">
            <div ml-bind="CCModuleImagePreview[item]:item">
                <span class="tools">
                    <span class="cc-icon cc-scratch-icon" title="Add to scratch" ml-bind="[events]:scratch"></span>
                </span>
                <img ml-bind="[data]:image">
            </div>
        </div>
    </div>
</div>
