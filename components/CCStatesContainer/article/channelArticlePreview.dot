<div ml-bind="CIPageItemArticle:{{= it.compName }}">
    <div class="article article-small">
        <h2 ml-bind="PIHeader:header">{{= it.title }}</h2>
        <div class="articletext">
            <a href="#">
                <img ml-bind="[data]:preview" src="{{= it.previewImg }}">
            </a>
            <p ml-bind="PIText:description">{{= it.previewText }}</p>
        </div>
    </div>
</div>
