{{# def.partials.formGroup }}
    {{# def.partials.label }}
    <div ml-bind="CCPreviewImage:{{= it.compName }}">
        <span class="tools">
            <span class="cc-icon cc-crop-icon" title="Crop image"></span>
        </span>
        <img ml-bind="[data]:image" {{? it.item.src }}src="{{= it.item.src }}"{{?}}
            {{? it.item.width }}width="{{= it.item.width }}"{{?}}
            {{? it.item.height }}height="{{= it.item.height }}"{{?}}
            {{? it.item.height }}style="max-width:300px !important; max-height:{{= 300 / it.item.width * it.item.height }}px !important"{{?}}
        >
    </div>
</div>
